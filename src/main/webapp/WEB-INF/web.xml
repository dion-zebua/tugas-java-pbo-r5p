<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>

    <welcome-file-list>
        <welcome-file>index</welcome-file>
    </welcome-file-list>





    <servlet>
        <servlet-name>LoginServlet</servlet-name>
        <servlet-class>com.myapp.servlet.LoginServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>LoginServlet</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>


    <servlet>
        <servlet-name>LoginSubmitServlet</servlet-name>
        <servlet-class>com.myapp.servlet.LoginSubmitServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>LoginSubmitServlet</servlet-name>
        <url-pattern>/login-post</url-pattern>
    </servlet-mapping>


    <servlet>
        <servlet-name>LogoutSubmitServlet</servlet-name>
        <servlet-class>com.myapp.servlet.LogoutSubmitServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>LogoutSubmitServlet</servlet-name>
        <url-pattern>/logout-post</url-pattern>
    </servlet-mapping>





    <servlet>
        <servlet-name>IndexServlet</servlet-name>
        <servlet-class>com.myapp.servlet.IndexServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>IndexServlet</servlet-name>
        <url-pattern>/index</url-pattern>
    </servlet-mapping>





    
    <servlet>
        <servlet-name>TransportationServlet</servlet-name>
        <servlet-class>com.myapp.servlet.TransportationServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>TransportationServlet</servlet-name>
        <url-pattern>/transportation</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>TransportationCreateServlet</servlet-name>
        <servlet-class>com.myapp.servlet.TransportationCreateServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>TransportationCreateServlet</servlet-name>
        <url-pattern>/transportation/create</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>TransportationDestroyServlet</servlet-name>
        <servlet-class>com.myapp.servlet.TransportationDestroyServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>TransportationDestroyServlet</servlet-name>
        <url-pattern>/transportation/destroy</url-pattern>
    </servlet-mapping>










    <servlet>
        <servlet-name>BookingServlet</servlet-name>
        <servlet-class>com.myapp.servlet.BookingServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>BookingServlet</servlet-name>
        <url-pattern>/booking</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>BookingCreateServlet</servlet-name>
        <servlet-class>com.myapp.servlet.BookingCreateServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>BookingCreateServlet</servlet-name>
        <url-pattern>/booking/create</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>BookingDestroyServlet</servlet-name>
        <servlet-class>com.myapp.servlet.BookingDestroyServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>BookingDestroyServlet</servlet-name>
        <url-pattern>/booking/destroy</url-pattern>
    </servlet-mapping>
</web-app>
